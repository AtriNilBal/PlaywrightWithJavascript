  },
  "homepage": "https://github.com/AtriNilBal/PlaywrightWithJavascript#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^12.3.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@cucumber/cucumber": "^12.3.0",
